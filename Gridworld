      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "from matplotlib.patches import Rectangle\n",
        "from PIL import Image\n",
        "import os\n",
        "\n",
        "def idx_to_xy(k, grid_n=3):\n",
        "    return divmod(k, grid_n)  # (row, col)\n",
        "\n",
        "def render_grid_frame(agent_idx, food_idx, grid_n=3, show_text=True):\n",
        "    import numpy as np\n",
        "    import matplotlib.pyplot as plt\n",
        "    from matplotlib.patches import Rectangle\n",
        "    from PIL import Image\n",
        "\n",
        "    fig, ax = plt.subplots(figsize=(3, 3), dpi=150)\n",
        "    ax.set_xlim(0, grid_n)\n",
        "    ax.set_ylim(0, grid_n)\n",
        "    ax.set_aspect('equal')\n",
        "    ax.invert_yaxis()\n",
        "\n",
        "    # draw grid lines\n",
        "    for g in range(grid_n + 1):\n",
        "        ax.plot([g, g], [0, grid_n], color=\"gray\", lw=0.5)\n",
        "        ax.plot([0, grid_n], [g, g], color=\"gray\", lw=0.5)\n",
        "\n",
        "    # food (outlined square)\n",
        "    fy, fx = divmod(food_idx, grid_n)\n",
        "    rect = Rectangle((fx, fy), 1, 1, fill=False, linewidth=2)\n",
        "    ax.add_patch(rect)\n",
        "    if show_text:\n",
        "        ax.text(fx + 0.5, fy + 0.6, \"F\", ha=\"center\", va=\"center\",\n",
        "                fontsize=16, fontweight=\"bold\", color=\"red\")\n",
        "\n",
        "    # agent (circle marker or label)\n",
        "    ay, axx = divmod(agent_idx, grid_n)\n",
        "    ax.text(axx + 0.5, ay + 0.5, \"A\", ha=\"center\", va=\"center\",\n",
        "            fontsize=16, fontweight=\"bold\", color=\"blue\")\n",
        "\n",
        "    ax.axis(\"off\")\n",
        "\n",
        "    # ---- FIXED PART ----\n",
        "    fig.canvas.draw()\n",
        "    # Extract RGB buffer safely for all backends\n",
        "    buf = np.frombuffer(fig.canvas.buffer_rgba(), dtype=np.uint8)\n",
        "    w, h = fig.canvas.get_width_height()\n",
        "    img = Image.frombytes(\"RGBA\", (w, h), buf)\n",
        "    plt.close(fig)\n",
        "    return img.convert(\"RGB\")\n",
        "\n",
        "\n",
        "def save_gridworld_gif(agent_path, food_idx, out_path, grid_n=3, fps=2):\n",
        "    frames = [render_grid_frame(a, food_idx, grid_n=grid_n) for a in agent_path]\n",
        "    duration_ms = int(1000 / max(1, fps))\n",
        "    frames[0].save(out_path, save_all=True, append_images=frames[1:], duration=duration_ms, loop=0)\n",
        "    return out_path"
      ],
      "metadata": {
        "id": "S_H19WpDhFV8"
      },
